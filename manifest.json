{
  "name": "Typoifier",
  "version": "1.0",
  "description": "A chrome extension to add Typos to websites",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "ContentScript.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts":["background.js"]
  },
  "manifest_version": 2
}
